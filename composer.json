{
    "name": "chewett/php-discord-webhook-messenger",
    "description": "A basic PHP library to simplify sending messages to Discord Webhook",
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": { "Chewett\\PHPDiscordWebhookMessenger\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chewett\\PHPDiscordWebhookMessenger\\Test\\": "tests/"}
    },
    "require": {
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "authors": [
        {
            "name": "Christopher Hewett",
            "email": "chewett@hotmail.co.uk"
        }
    ]
